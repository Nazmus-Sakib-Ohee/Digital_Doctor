<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.Nazmus_Sakib_Ohee.digital_doctor.MapsActivity">
    <Button
        android:layout_margin="5dp"
        android:id="@+id/btn1"
        android:layout_marginTop="5dp"
        android:layout_width="match_parent"
        android:text="Get Hospital Locations"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="20dp"
        android:background="@drawable/appoint_round"
        android:layout_height="wrap_content" />

    <fragment
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.aukawser.myapplication.MapsActivity" />


</LinearLayout>
